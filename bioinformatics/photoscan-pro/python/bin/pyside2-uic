#!/opt/python3.5/bin/python3.5m
# EASY-INSTALL-ENTRY-SCRIPT: 'PySide2==2.0.0.dev0','console_scripts','pyside2-uic'
__requires__ = 'PySide2==2.0.0.dev0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('PySide2==2.0.0.dev0', 'console_scripts', 'pyside2-uic')()
    )
